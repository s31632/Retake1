public class Main {
    public static void main(String[] args) {
        Marina marina1 = new Marina("Name","Capacity","Adress","Watercraft<Arraylist>");
        Watercraft watercraft1 = new Watercraft("registration number","color","model");      Yacht yacht1 = new Yacht;
        Jetski jetski1 = new Jetski;("enginePower");
        Yacht yacht1 = new Yacht;("length","luxuryLevel");
        Boat boat1 = new Boat;("boatType","capasity");
    }
